{
  "name": "xtratiler",
  "version": "0.9.10",
  "description": "raster tile renderer for ldproxy",
  "author": "interactive instruments GmbH",
  "license": "MIT",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "v": "node --version",
    "build": "node build.js",
    "start": "npm run build && node ./build/bin/index.cjs",
    "dev": "tsx ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf build && rimraf dist",
    "presea": "npm run build && node dist.js --pre && node --experimental-sea-config $(node dist.js --sea-path)",
    "preseamac": "npm run presea && codesign --remove-signature $(node dist.js --bin-path)",
    "sea": "node dist.js",
    "seamac": "node dist.js --mac && codesign --sign - $(node dist.js --bin-path)",
    "docker": "npm run sea && node docker.js",
    "predocs": "echo '#!/bin/sh\\nnpm run --silent dev -- \"$@\"' > ./build/xt.sh && chmod +x ./build/xt.sh",
    "docs": "cli-docs-generator --cli=./build/xt.sh --pretty_cli_name=xtratiler --output=./docs/COMMANDS.md -v"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "0.57.1",
    "@opentelemetry/exporter-trace-otlp-proto": "0.57.1",
    "@opentelemetry/resources": "1.30.1",
    "@opentelemetry/sdk-metrics": "1.30.1",
    "@opentelemetry/sdk-node": "0.57.1",
    "@opentelemetry/sdk-trace-node": "1.30.1",
    "@opentelemetry/semantic-conventions": "1.28.0",
    "@tsconfig/node20": "20.1.4",
    "@types/js-yaml": "4.0.9",
    "@types/make-fetch-happen": "10.0.4",
    "@types/mapbox__sphericalmercator": "1.2.3",
    "@types/node": "^20.17.16",
    "@types/pretty-time": "1.1.5",
    "@types/yargs": "17.0.33",
    "cli-docs-generator": "1.0.7",
    "esbuild": "0.24.2",
    "esbuild-plugin-clean": "1.0.1",
    "esbuild-plugin-copy": "2.1.1",
    "esbuild-plugin-replace": "1.4.0",
    "fs-extra": "11.3.0",
    "node": "^20.18.2",
    "postject": "^1.0.0-alpha.6",
    "rimraf": "6.0.1",
    "tsx": "4.19.2",
    "type-fest": "4.33.0",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@inquirer/confirm": "5.1.3",
    "@mapbox/jsonlint-lines-primitives": "2.0.2",
    "@mapbox/mbtiles": "0.12.1",
    "@mapbox/sphericalmercator": "2.0.1",
    "@maplibre/maplibre-gl-native": "6.0.0",
    "@maplibre/maplibre-gl-style-spec": "23.1.0",
    "filehound": "1.17.6",
    "js-yaml": "4.1.0",
    "make-fetch-happen": "14.0.3",
    "modern-async": "2.0.4",
    "pino": "9.6.0",
    "pino-pretty": "13.0.0",
    "pino-roll": "2.2.0",
    "pretty-time": "1.1.0",
    "sharp": "0.33.5",
    "yargs": "17.7.2"
  },
  "resolutions": {
    "sqlite3": "5.1.7"
  }
}
